<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="640.0" prefWidth="921.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="guiController">
   <children>
      <TitledPane animated="false" layoutX="22.0" layoutY="14.0" prefHeight="76.0" prefWidth="863.0" text="Config">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="61.0" prefWidth="861.0">
               <children>
                  <Button layoutX="37.0" layoutY="2.0" mnemonicParsing="false" onAction="#select_main_image_folder" text="Select Main Image Folder" />
                  <Button layoutX="354.0" layoutY="2.0" mnemonicParsing="false" onAction="#select_good_image_folder" text="Select Good Image Folder" />
                  <Button layoutX="695.0" layoutY="2.0" mnemonicParsing="false" onAction="#select_bad_image_folder" text="Select Bad Image Folder" />
                  <Label fx:id="main_image_folder_label" layoutX="37.0" layoutY="31.0" />
                  <Label fx:id="good_image_folder_label" layoutX="354.0" layoutY="31.0" />
                  <Label fx:id="bad_image_folder_label" layoutX="695.0" layoutY="31.0" />
               </children>
            </AnchorPane>
        </content>
      </TitledPane>
      <TitledPane animated="false" layoutX="22.0" layoutY="94.0" prefHeight="449.0" prefWidth="863.0" text="Image">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="413.0" prefWidth="842.0">
               <children>
                  <Button fx:id="start_slide_show_bnt" layoutX="380.0" layoutY="187.0" mnemonicParsing="false" onAction="#start_slide_show" text="Start Slide Show" />
                  <ImageView fx:id="image_view" fitHeight="407.0" fitWidth="852.0" layoutX="6.0" layoutY="12.0" pickOnBounds="true" preserveRatio="true" />
               </children>
            </AnchorPane>
        </content>
      </TitledPane>
      <TitledPane animated="false" layoutX="24.0" layoutY="558.0" prefHeight="67.0" prefWidth="863.0" text="Select">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="31.0" prefWidth="293.0">
               <children>
                  <Button layoutX="14.0" layoutY="5.0" mnemonicParsing="false" onAction="#select_good" prefHeight="25.0" prefWidth="208.0" style="-fx-background-color: green;" text="Good" textFill="#f8f7f7" />
                  <Button layoutX="630.0" layoutY="8.0" mnemonicParsing="false" onAction="#select_bad" prefHeight="25.0" prefWidth="210.0" style="-fx-background-color: red;" text="Bad" textFill="WHITE" />
                  <Label fx:id="end" layoutX="363.0" layoutY="9.0" text="End Of Slide Show" />
               </children>
            </AnchorPane>
        </content>
      </TitledPane>
   </children>
</AnchorPane>
